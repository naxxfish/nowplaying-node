<form action="/feed" method="post">
<div>
<label>secret:</label>
<input type="text" name="feedSecret"/><br/>
</div>
<div>
<label>Set track data?</label>
<input type="checkbox" name="setTrack" />
</div>
<div>
<label>artist</label>
<input type="text" name="track.artist"/>
</div>
<div>
<label>title</label>
<input type="text" name="track.title"/>
</div>

<div>
<label>Set show data?</label>
<input type="checkbox" name="setShow" />
</div>
<div>
<label>Show Name</label>
<input type="text" name="show.name"/>
</div>
<div>
<label>Show presenter</label>
<input type="text" name="show.presenter"/>
</div>
<div>
<label>Show URL</label>
<input type="text" name="show.url"/>
</div>
<div>
<input type="submit" value="Submit"/>
</div>
</form>